option_settings:
  aws:elasticbeanstalk:container:dotnet:apppool:
    Enable 32Bit Applications: false
  aws:elasticbeanstalk:application:environment:
    ASPNETCORE_ENVIRONMENT: Production
    AWS_REGION: us-east-1
    # Add your RDS connection string
    RDS_CONNECTION_STRING: "Server=your-rds-endpoint.rds.amazonaws.com;Database=PodcastDB;User Id=admin;Password=your-password;TrustServerCertificate=true;"